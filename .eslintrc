{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint", "prettier"],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "rules": {
        "operator-linebreak": 0,
        "import/extensions": 0,
        "consistent-return": 0,
        "func-names": 0,
        "comma-dangle": 0,
        "no-console": 0,
        "indent": 0,
        "linebreak-style": 0,
        "new-cap": 0,
        "object-curly-newline": 0,
        "prettier/prettier": [
            "warning",
            {
                "trailingComma": "es5",
                "printWidth": 100,
                "tabWidth": 4,
                "endOfLine": "auto",
                "semi": true
            }
        ]
    }
}
